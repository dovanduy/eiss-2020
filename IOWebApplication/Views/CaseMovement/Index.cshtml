@using IOWebApplication.Infrastructure.Models.ViewModels.Common;

@{
    var caseId = ViewBag.caseId;
    ViewData["Title"] = "Местоположение на дело " + ViewBag.CaseName;
}

@section breadcrumb{
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")" title="Начало"><i class="fa fa-home"></i></a></li>
        <li><a href="@Url.Action("Index","Case")">Съдебни дела</a></li>
        <li><a href="@Url.Action("CasePreview","Case", new { id = caseId})">Дело @ViewBag.CaseName</a></li>
    </ol>
}

<div class="row">
    <div class="col-md-12">
        <partial name="../CaseMovement/_CaseMovement.cshtml" model="@new SourceTypeSelectVM { SourceType = caseId} " />
    </div>
</div>

